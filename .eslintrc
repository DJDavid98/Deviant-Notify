{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "import-newlines"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": [
      "error",
      "always"
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "max-len": [
      "error",
      120
    ],
    "default-case": "off",
    "no-nested-ternary": "off",
    "no-void": "off",
    "no-plusplus": "off",
    "import-newlines/enforce": [
      "error",
      4,
      120
    ],
    "object-curly-newline": [
      "error",
      {
        "multiline": true,
        "minProperties": 5,
        "consistent": true
      }
    ]
  },
  "globals": {
    "$": "readonly",
    "browser": "readonly",
    "chrome": "readonly"
  }
}
